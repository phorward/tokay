Until : @<P, Escape: Void, empty: false> {
    Peek<P>  if empty accept $0 else if $0 accept $0 else reject
    (Escape P | Char)  repeat
    reject
}

String: @<Start: Char<'">, End: Void, Escape: '\\'> {
    Start Until<(End | Start), Escape, empty: true> Match($1)  $2
}

